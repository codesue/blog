{
	"name": "codesue",
	"version": "1.0.0",
	"description": "Blog of Suzen Fylke.",
	"author": {
		"name": "Suzen Fylke",
		"url": "https://suzenfylke.com/"
	},
	"homepage": "https://github.com/codesue/blog#readme",
	"repository": {
		"type": "git",
		"url": "git://github.com/codesue/blog.git"
	},
	"bugs": {
		"url": "https://github.com/codesue/blog/issues"
	},
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"start": "npm run dev",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"build": "npx @11ty/eleventy",
		"clean": "del-cli dist",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"dev": "npx @11ty/eleventy --serve --quiet",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "echo \"Error: no tests specified\" && exit 1"
	},
	"devDependencies": {
		"@11ty/eleventy": "2.0.0-beta.2",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.1",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"del-cli": "^5.0.0",
		"eslint-config-wesbos": "^3.2.3",
		"luxon": "^3.2.1",
		"markdown-it-anchor": "^8.6.6"
	},
	"eslintConfig": {
		"extends": [
			"wesbos"
		],
		"rules": {
			"import/no-extraneous-dependencies": "off",
			"global-require": "off",
			"quotes": [
				"error",
				"double"
			],
			"prettier/prettier": [
				"error",
				{
					"singleQuote": false
				}
			]
		}
	}
}
